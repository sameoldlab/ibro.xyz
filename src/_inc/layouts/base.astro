---
import '../css/main.css'
import Nav from '../components/nav.astro'
import Meta from '../components/meta.astro'
import  SpeedInsights  from "@vercel/speed-insights/astro"
export interface Props {
	title: string
	description?: string
	type?: 'website' | {
		published_time: Date
		modified_time: Date
		tags: string[]
	}
	image?: {
		url: string
		url_fallback?: string
		width: number
		height: number
	}
}
const props = Astro.props
